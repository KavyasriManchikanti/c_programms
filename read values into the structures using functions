#include <stdio.h>
struct node{
        char title[10];
        char author[10];
    };
 void read(struct node *s){
    scanf("%s",s->title);
    scanf("%s",s->author);
}
void display(struct node s){
    printf("%s\n",s.title);
    printf("%s\n",s.author);
}
int main()
{
    struct node s1;
    read(&s1);
    display(s1);
    return 0;
}
